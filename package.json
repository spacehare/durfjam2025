{
	"scripts": {
		"build:tailwindcss": "tailwindcss -i ./ocs/ocs.css -o ./out/out.css",
		"build:postcss": "postcss ./ocs/ocs.css -o ./out/out.css",
		"build:pdf": "py -m ocs.ocs",
		"build": "bun run build:postcss && bun run build:pdf"
	},
	"dependencies": {
		"@tailwindcss/postcss": "^4.1.14",
		"postcss": "^8.5.6",
		"postcss-calc": "^10.1.1",
		"postcss-preset-env": "^10.4.0",
		"tailwindcss": "^4.1.14"
	},
	"devDependencies": {
		"postcss-cli": "^11.0.1"
	}
}
